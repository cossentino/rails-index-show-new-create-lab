
<h1>Coupon Form</h1>

<%= form_tag coupons_path do %>
    <label for="store">Store: </label>
    <input type="text" name="coupon[store]" id="coupon[store]">

    <label for="coupon_code">Coupon Code: </label>
    <input type="text" name="coupon[coupon_code]" id="coupon[coupon_code]">

    <%= submit_tag "Submit Coupon" %>
<% end %>